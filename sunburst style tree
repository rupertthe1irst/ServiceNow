import matplotlib.pyplot as plt
import matplotlib
import pandas as pd

matplotlib.use('Agg')

df = dataset.copy()
df['Created'] = pd.to_datetime(df['Created'], errors='coerce')

fig, ax = plt.subplots(figsize=(10, 4))
ax.axis('off')

sample = df['Created'].dropna().head(10).astype(str).tolist()
text = "Raw Created values Power BI is passing:\n\n" + "\n".join(sample)
ax.text(0.05, 0.95, text, transform=ax.transAxes, fontsize=11, verticalalignment='top', fontfamily='monospace')

plt.tight_layout()
plt.show()
